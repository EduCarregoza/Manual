<?xml version='1.0' encoding='utf-8'?>
<widget id="br.com.meuApp.www" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Manual do Recruta</name>
    <description>
        Esse app tem como objetivo ajudar aos novos militares temporarios e de carreira.Onde iram encontrar fotos,canções,vídeos,História e tudo sobre a semana zero.
			 com fundamento de mostrar aos  novos combatentes oque eles iram enfrentar no periodo Basico.
    </description>
    <author email="support@phonegap.com" href="http://phonegap.com">
        PhoneGap Team
    </author>
    <content src="index.html" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
	<plugin name="cordova-plugin-whitelist" source="npm" spec="~1.2.1" />
   
    <platform name="android">
        <icon density="ldpi" src="www/icon/mdpi.png" />
        <icon density="mdpi" src="www/icon/mdpi.png" />
        <icon density="hdpi" src="www/icon/hdpi.png" />
        <icon density="xhdpi" src="www/icon/xhdpi.png" />
        <icon density="xxhdpi" src="www/icon/xxhdpi.png" />
        <icon density="xxxhdpi" src="www/icon/xxxhdpi.png" />
        
    </platform>
    
    <access origin="*" />
	<allow-navigation href="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    
</widget>
